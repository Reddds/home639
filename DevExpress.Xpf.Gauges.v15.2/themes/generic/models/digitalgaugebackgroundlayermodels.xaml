  <Style TargetType="{x:Type local:DefaultDigitalGaugeBackgroundLayerControl}">
    <Setter Property="Control.Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type local:DefaultDigitalGaugeBackgroundLayerControl}">
          <Grid>
            <Rectangle Margin="0" Fill="#FFB0B0B0" RadiusX="18" RadiusY="18"/>
            <Rectangle Margin="1" StrokeThickness="0" Fill="#FFEEEEEE" RadiusX="17" RadiusY="17"/>
            <Rectangle StrokeThickness="0" Fill="#FFB0B0B0" Margin="10" RadiusX="8" RadiusY="8"/>
            <Rectangle Margin="11" StrokeThickness="0" RadiusX="7" RadiusY="7"
                       Fill="{Binding Path=Presentation.ActualFill}"/>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
