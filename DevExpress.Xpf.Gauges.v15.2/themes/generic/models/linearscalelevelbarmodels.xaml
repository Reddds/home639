              <Shape.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                  <GradientStop Color="#FFCED2DE" Offset="0"/>
                  <GradientStop Color="#FFA5AABA" Offset="1"/>
                </LinearGradientBrush>
              </Shape.Fill>
            </Rectangle>
            <Rectangle Fill="#FFF1F3F6" Margin="0,1" StrokeThickness="0" HorizontalAlignment="Center" Width="8"
                       RadiusX="4" RadiusY="4"/>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
