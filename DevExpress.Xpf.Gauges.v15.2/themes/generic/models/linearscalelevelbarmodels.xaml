                       Fill="{Binding Path=Presentation.ActualFill}"/>
            <Ellipse Height="12" StrokeThickness="0" VerticalAlignment="Bottom" Width="12" Margin="5,0,5,5"
                     Fill="{Binding Path=Presentation.ActualFill}"/>
          </Grid>
        </ControlTemplate>
