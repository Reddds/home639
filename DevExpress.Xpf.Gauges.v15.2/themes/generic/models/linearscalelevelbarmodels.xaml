<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DevExpress.Xpf.Gauges"
                    xmlns:core="http://schemas.devexpress.com/winfx/2008/xaml/core">
  <Style TargetType="{x:Type local:DefaultLinearScaleLevelBarBackgroundControl}">
    <Setter Property="Control.Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type local:DefaultLinearScaleLevelBarBackgroundControl}">
          <Grid RenderTransformOrigin="0.5, 1" Margin="0,-12">
            <Rectangle Fill="#FFB0B0B0" StrokeThickness="0" HorizontalAlignment="Center" Width="10" RadiusX="5"
                       RadiusY="5"/>
            <Rectangle Fill="#FFF7F7F7" Margin="0,1" StrokeThickness="0" HorizontalAlignment="Center" Width="8"
                       RadiusX="4" RadiusY="4"/>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="{x:Type local:DefaultLinearScaleLevelBarForegroundControl}">
    <Setter Property="Control.Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type local:DefaultLinearScaleLevelBarForegroundControl}">
          <Rectangle RenderTransformOrigin="0.5, 1" Margin="0,-10" StrokeThickness="0" HorizontalAlignment="Center"
                     Width="6" RadiusX="3" RadiusY="3" Fill="{Binding Path=Presentation.ActualFill}"/>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
