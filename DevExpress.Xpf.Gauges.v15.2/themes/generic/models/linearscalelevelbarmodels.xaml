  <Style TargetType="{x:Type local:RedThermometerLinearScaleLevelBarBackgroundControl}">
    <Setter Property="Control.Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type local:RedThermometerLinearScaleLevelBarBackgroundControl}">
          <Grid RenderTransformOrigin="0.5, 1" Margin="0,-12,0,-19" Width="22">
            <Ellipse Fill="White" Height="22" StrokeThickness="0" VerticalAlignment="Bottom" Width="22" Margin="0"/>
            <Rectangle StrokeThickness="0" Width="14" RadiusX="7" RadiusY="7" Fill="White" Margin="0,0,0,5"/>
            <Rectangle StrokeThickness="0" Width="12" RadiusX="6" RadiusY="6" Margin="0,1,0,5">
              <Shape.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                  <GradientStop Color="#FFADB0BE" Offset="0"/>
                  <GradientStop Color="#FF88889D" Offset="1"/>
                </LinearGradientBrush>
              </Shape.Fill>
            </Rectangle>
            <Ellipse Fill="#FF898A9E" Height="20" StrokeThickness="0" VerticalAlignment="Bottom" Width="20"
                     Margin="1,0,1,1"/>
            <Ellipse Fill="#FFBFBFCE" Height="18" StrokeThickness="0" VerticalAlignment="Bottom" Width="18" Margin="2"/>
            <Rectangle Margin="1,2,1,6" StrokeThickness="0" RadiusX="5" RadiusY="5" Width="10">
              <Shape.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                  <GradientStop Color="#FFD3D5DE" Offset="0"/>
                  <GradientStop Color="#FFBFBFCE" Offset="1"/>
                </LinearGradientBrush>
              </Shape.Fill>
            </Rectangle>
            <Rectangle Margin="6,3,6,6" StrokeThickness="0" RadiusX="4" RadiusY="4" Width="8">
              <Shape.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                  <GradientStop Color="#FFE3E5EC" Offset="0"/>
                  <GradientStop Color="#FFD8D9E1" Offset="1"/>
                </LinearGradientBrush>
              </Shape.Fill>
            </Rectangle>
            <Ellipse Fill="#FFD8D8E1" Height="16" StrokeThickness="0" VerticalAlignment="Bottom" Width="16"
                     Margin="3,0,3,3"/>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
