  <Style TargetType="{x:Type local:DefaultArcScaleBackgroundLayerControl}">
    <Setter Property="Control.Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type local:DefaultArcScaleBackgroundLayerControl}">
          <Grid RenderTransformOrigin="0.5, 0.5" Margin="0">
            <Ellipse Margin="0" Fill="#FFB0B0B0"/>
            <Ellipse Margin="1" StrokeThickness="0" Fill="#FFEEEEEE"/>
            <Ellipse StrokeThickness="0" Fill="#FFB0B0B0" Margin="12"/>
            <Ellipse Margin="13" StrokeThickness="0" Fill="{Binding Path=Presentation.ActualFill}"/>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
