  <Style TargetType="{x:Type local:CleanWhiteArcScaleBackgroundLayerControl}">
    <Setter Property="Control.Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type local:CleanWhiteArcScaleBackgroundLayerControl}">
          <Grid RenderTransformOrigin="0.5, 0.5">
            <Ellipse Margin="2,4,2,0" Opacity="0.15">
              <Shape.Fill>
                <RadialGradientBrush>
                  <GradientStop Color="Black" Offset="0"/>
                  <GradientStop Color="#00000000" Offset="1"/>
                  <GradientStop Color="Black" Offset="0.96"/>
                </RadialGradientBrush>
              </Shape.Fill>
              <UIElement.OpacityMask>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                  <GradientStop Color="Black" Offset="0"/>
                  <GradientStop Color="White" Offset="1"/>
                </LinearGradientBrush>
              </UIElement.OpacityMask>
            </Ellipse>
            <Grid Margin="3">
              <Grid>
                <Ellipse Margin="0">
                  <Shape.Fill>
                    <RadialGradientBrush Center="0.5,1" GradientOrigin="0.5,1" RadiusY="1" RadiusX="1">
                      <Brush.RelativeTransform>
                        <RotateTransform CenterY="1" CenterX="0.5" Angle="-45"/>
                      </Brush.RelativeTransform>
                      <GradientStop Color="#FFCCCED5" Offset="1"/>
                      <GradientStop Color="#FFA1A9C3"/>
                    </RadialGradientBrush>
                  </Shape.Fill>
                </Ellipse>
                <Ellipse Margin="1" StrokeThickness="0">
                  <Shape.Fill>
                    <RadialGradientBrush Center="0.5,1" GradientOrigin="0.5,1" RadiusY="1" RadiusX="1">
                      <Brush.RelativeTransform>
                        <RotateTransform CenterY="1" CenterX="0.5" Angle="-45"/>
                      </Brush.RelativeTransform>
                      <GradientStop Color="#FFCDCFD6" Offset="1"/>
                      <GradientStop Color="#FFF4F5F6"/>
                    </RadialGradientBrush>
                  </Shape.Fill>
                </Ellipse>
                <Ellipse Margin="1" StrokeThickness="0">
                  <Shape.Fill>
                    <RadialGradientBrush Center="1.206,0.293" GradientOrigin="1.206,0.293" RadiusY="1" RadiusX="1">
                      <Brush.RelativeTransform>
                        <RotateTransform CenterY="1" CenterX="0.5" Angle="-45"/>
                      </Brush.RelativeTransform>
                      <GradientStop Color="#00F4F5F6" Offset="0.65"/>
                      <GradientStop Color="#FFF4F5F6"/>
                    </RadialGradientBrush>
                  </Shape.Fill>
                </Ellipse>
                <Grid Margin="1">
                  <Grid.RowDefinitions>
                    <RowDefinition Height="0.7*"/>
                    <RowDefinition Height="0.3*"/>
                  </Grid.RowDefinitions>
                  <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.1*"/>
                    <ColumnDefinition Width="0.8*"/>
                    <ColumnDefinition Width="0.1*"/>
                  </Grid.ColumnDefinitions>
                  <Ellipse StrokeThickness="0" Grid.Column="1" Margin="0">
                    <Shape.Fill>
                      <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="White" Offset="0"/>
                        <GradientStop Color="Transparent" Offset="0.5"/>
                      </LinearGradientBrush>
                    </Shape.Fill>
                  </Ellipse>
                </Grid>
                <Ellipse Margin="18" StrokeThickness="0">
                  <Shape.Fill>
                    <RadialGradientBrush Center="0.501,0.004" GradientOrigin="0.501,0.004" RadiusY="0.995" RadiusX="0.995">
                      <GradientStop Color="#FF8F95A9"/>
                      <GradientStop Color="White" Offset="1"/>
                    </RadialGradientBrush>
                  </Shape.Fill>
                </Ellipse>
              </Grid>
              <Grid Margin="21">
                <Ellipse StrokeThickness="0">
                  <Shape.Fill>
                    <RadialGradientBrush RadiusY="1.016" RadiusX="1.016" Center="0.5,-0.016" GradientOrigin="0.5,-0.016">
                      <GradientStop Color="#FF8088A1" Offset="0"/>
                      <GradientStop Color="#FFBFC6DA" Offset="1"/>
                    </RadialGradientBrush>
                  </Shape.Fill>
                </Ellipse>
                <Ellipse Margin="1" StrokeThickness="0" Fill="{Binding Path=Presentation.ActualFill}"/>
                <Grid Margin="3">
                  <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.15*"/>
                    <ColumnDefinition Width="0.7*"/>
                    <ColumnDefinition Width="0.15*"/>
                  </Grid.ColumnDefinitions>
                  <Grid.RowDefinitions>
                    <RowDefinition Height="0.5*"/>
                    <RowDefinition Height="0.5*"/>
                  </Grid.RowDefinitions>
                  <Ellipse StrokeThickness="0" Margin="0" Grid.Column="1">
                    <Shape.Fill>
                      <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="White" Offset="0"/>
                        <GradientStop Color="Transparent" Offset="1"/>
                      </LinearGradientBrush>
                    </Shape.Fill>
                  </Ellipse>
                </Grid>
                <Ellipse Margin="0" StrokeThickness="0" Opacity="0.1">
                  <Shape.Fill>
                    <RadialGradientBrush RadiusY="0.519" RadiusX="0.519">
                      <GradientStop Color="#FF8088A1" Offset="1"/>
                      <GradientStop Offset="0.9"/>
                    </RadialGradientBrush>
                  </Shape.Fill>
                </Ellipse>
              </Grid>
            </Grid>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
