                            <Style TargetType="{x:Type TextBlock}">
                                <Setter Property="Foreground" Value="White"/>
                                <Setter Property="Margin" Value="0,60,0,0" />
                                <Setter Property="FontSize" Value="16"/>
                            </Style>
                        </wpfGauge:Gauge.ValueTextStyle>

                        <wpfGauge:Gauge.TickLabelStyle>
                            <Style TargetType="{x:Type TextBlock}">
                                <Setter Property="Foreground" Value="White"/>
                                <Setter Property="FontSize" Value="12"/>
                            </Style>
                        </wpfGauge:Gauge.TickLabelStyle>

                    </wpfGauge:Gauge>
                    <wpfGauge:Gauge 
						FontSize="12" AutoScale="False" Value="20"
						x:Name="GBedroomHym" MajorTickCount="10" MinorTickCount="5" MinValue="0" MaxValue="100" 
						HorizontalAlignment="Left" StartAngle="-65" EndAngle="247" MajorTickMarkColor="LightYellow" Margin="268,88,0,0" Grid.Row="0" VerticalAlignment="Top" Height="200" Width="200">
                        <wpfGauge:Gauge.GoalMarkers>
                            <wpfGauge:GoalMarker MarkerColor="Red" Value="30" />
                        </wpfGauge:Gauge.GoalMarkers>
