                    <dxga:CircularGaugeControl x:Name="GBedroomTemp" Width="200" Height="200" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="23,38,0,0">
                        <dxga:CircularGaugeControl.Model>
                            <dxga:CircularEcoModel/>
                        </dxga:CircularGaugeControl.Model>
                        <dxga:CircularGaugeControl.Scales>
                            <dxga:ArcScale StartValue="10" EndValue="35" 
								StartAngle="-230" EndAngle="50" MinorIntervalCount="5" MajorIntervalCount="5">

                                <dxga:ArcScale.CustomElements>
                                    <dxga:ScaleCustomElement VerticalAlignment="Bottom">
                                        <Image Source="Assets/Images/Objects/Thermometre.png" Height="30" Margin="0,0,0,30" />
                                    </dxga:ScaleCustomElement>
                                </dxga:ArcScale.CustomElements>
                                <dxga:ArcScale.LabelOptions>
                                    <dxga:ArcScaleLabelOptions Orientation="LeftToRight" />
                                </dxga:ArcScale.LabelOptions>
                                <dxga:ArcScale.Needles>
                                    <dxga:ArcScaleNeedle x:Name="BedroomTempNeedle"  Value="{Binding BedroomTemp}" />
                                </dxga:ArcScale.Needles>
                                <dxga:ArcScale.Markers>
                                    <dxga:ArcScaleMarker Value="20" />
                                </dxga:ArcScale.Markers>
                                <dxga:ArcScale.RangeBars>
                                    <dxga:ArcScaleRangeBar AnchorValue="24" Value="18" />
                                </dxga:ArcScale.RangeBars>
                                <dxga:ArcScale.Layers>
                                    <dxga:ArcScaleLayer />
                                </dxga:ArcScale.Layers>
                                <dxga:ArcScale.Ranges>
                                    <dxga:ArcScaleRange StartValue="18" EndValue="24" />
                                    <dxga:ArcScaleRange StartValue="10" EndValue="18" />
                                    <dxga:ArcScaleRange StartValue="24" EndValue="35" />
                                </dxga:ArcScale.Ranges>
                            </dxga:ArcScale>
                        </dxga:CircularGaugeControl.Scales>
                    </dxga:CircularGaugeControl>
                    <dxga:StateIndicatorControl dxga:AnalogGaugeControl.ValueIndicator="{Binding ElementName=BedroomTempNeedle}"  
						HorizontalAlignment="Left" Margin="201,279,0,0"  
						VerticalAlignment="Top" Height="32" Width="32" >
                        <dxga:StateIndicatorControl.AdditionalStates>
                            <dxga:State>
                                <dxga:State.Presentation>
                                    <dxga:LampGreenStatePresentation />
                                </dxga:State.Presentation>
                            </dxga:State>
                            <dxga:State>
                                <dxga:State.Presentation>
                                    <dxga:LampYellowStatePresentation />
                                </dxga:State.Presentation>
                            </dxga:State>
                            <dxga:State>
                                <dxga:State.Presentation>
                                    <dxga:LampRedStatePresentation />
                                </dxga:State.Presentation>
                            </dxga:State>
                        </dxga:StateIndicatorControl.AdditionalStates>
                    </dxga:StateIndicatorControl>



                    <dxga:CircularGaugeControl x:Name="GBedroomHym" Width="200"  Height="200" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="276,38,0,0">
                        <dxga:CircularGaugeControl.Model>
                            <dxga:CircularEcoModel/>
                        </dxga:CircularGaugeControl.Model>

                        <dxga:CircularGaugeControl.Scales>
                            <dxga:ArcScale StartValue="0" EndValue="100" 
								StartAngle="-230" EndAngle="50" MinorIntervalCount="5">
                                <dxga:ArcScale.LabelOptions>
                                    <dxga:ArcScaleLabelOptions Orientation="LeftToRight" />
                                </dxga:ArcScale.LabelOptions>
                                <dxga:ArcScale.CustomElements>
                                    <dxga:ScaleCustomElement VerticalAlignment="Bottom">
                                        <Image Source="Assets/Images/Objects/Humidity.png"  Height="30" Margin="0,0,0,30" />
                                    </dxga:ScaleCustomElement>
                                </dxga:ArcScale.CustomElements>

                                <dxga:ArcScale.Needles>
                                    <dxga:ArcScaleNeedle x:Name="BedroomHymNeedle"  Value="{Binding BedroomHym}" />
                                </dxga:ArcScale.Needles>
                                <dxga:ArcScale.Markers>
                                    <dxga:ArcScaleMarker Value="50" />
                                </dxga:ArcScale.Markers>
                                <dxga:ArcScale.RangeBars>
                                    <dxga:ArcScaleRangeBar AnchorValue="60" Value="30" />
                                </dxga:ArcScale.RangeBars>
                                <dxga:ArcScale.Layers>
                                    <dxga:ArcScaleLayer />
                                </dxga:ArcScale.Layers>
                                <dxga:ArcScale.Ranges>
                                    <dxga:ArcScaleRange StartValue="30" EndValue="60" />
                                    <dxga:ArcScaleRange StartValue="60" EndValue="100" />
                                    <dxga:ArcScaleRange StartValue="0" EndValue="30" />
                                </dxga:ArcScale.Ranges>
                            </dxga:ArcScale>
                        </dxga:CircularGaugeControl.Scales>
                    </dxga:CircularGaugeControl>
                    <dxga:StateIndicatorControl dxga:AnalogGaugeControl.ValueIndicator="{Binding ElementName=BedroomHymNeedle}"  
						HorizontalAlignment="Left" Margin="456,279,0,0"  
						VerticalAlignment="Top" Height="32" Width="32" >
                        <dxga:StateIndicatorControl.AdditionalStates>
                            <dxga:State>
                                <dxga:State.Presentation>
                                    <dxga:LampGreenStatePresentation />
                                </dxga:State.Presentation>
                            </dxga:State>
                            <dxga:State>
                                <dxga:State.Presentation>
                                    <dxga:LampYellowStatePresentation />
                                </dxga:State.Presentation>
                            </dxga:State>
                            <dxga:State>
                                <dxga:State.Presentation>
                                    <dxga:LampRedStatePresentation />
                                </dxga:State.Presentation>
                            </dxga:State>
                        </dxga:StateIndicatorControl.AdditionalStates>
                    </dxga:StateIndicatorControl>




                    <dxga:DigitalGaugeControl x:Name="digitalGaugeControl1"
						Text="{Binding BedroomTemp}" HorizontalAlignment="Left" VerticalAlignment="Top"
						Height="100" Width="150" Padding="7" Margin="51,243,0,0">
                        <!--region #Layers-->
                        <dxga:DigitalGaugeControl.Layers>
                            <dxga:DigitalGaugeLayer>
                                <dxga:DigitalGaugeLayer.Presentation>
                                    <dxga:EcoDigitalGaugeBackgroundLayerPresentation/>
                                </dxga:DigitalGaugeLayer.Presentation>
                            </dxga:DigitalGaugeLayer>
                        </dxga:DigitalGaugeControl.Layers>
                        <!--endregion #Layers-->
                        <!--region #Model-->
                        <dxga:DigitalGaugeControl.Model>
                            <dxga:DigitalEcoModel/>
                        </dxga:DigitalGaugeControl.Model>
                        <!--endregion #Model-->
                        <dxga:DigitalGaugeControl.SymbolView>
                            <dxga:SevenSegmentsView/>
                        </dxga:DigitalGaugeControl.SymbolView>
                    </dxga:DigitalGaugeControl>
                    <dxga:DigitalGaugeControl x:Name="digitalGaugeControl1_Copy"
						Text="{Binding BedroomHym}" HorizontalAlignment="Left" VerticalAlignment="Top"
						Height="100" Width="150" Padding="7" Margin="301,243,0,0">
                        <!--endregion #Layers-->
                        <!--region #Layers-->
                        <dxga:DigitalGaugeControl.Layers>
                            <dxga:DigitalGaugeLayer>
                                <dxga:DigitalGaugeLayer.Presentation>
                                    <dxga:EcoDigitalGaugeBackgroundLayerPresentation/>
                                </dxga:DigitalGaugeLayer.Presentation>
                            </dxga:DigitalGaugeLayer>
                        </dxga:DigitalGaugeControl.Layers>
                        <!--region #Model-->
                        <dxga:DigitalGaugeControl.Model>
                            <dxga:DigitalEcoModel/>
                        </dxga:DigitalGaugeControl.Model>
                        <!--endregion #Model-->
                        <dxga:DigitalGaugeControl.SymbolView>
                            <dxga:SevenSegmentsView/>
                        </dxga:DigitalGaugeControl.SymbolView>
                    </dxga:DigitalGaugeControl>
                    <dxga:DigitalGaugeControl x:Name="DgcLastPooTime"
						Text="{Binding LastPooTime, Source={x:Static properties:Settings.Default}, StringFormat=dd MMM HH:mm}" HorizontalAlignment="Left" VerticalAlignment="Top"
						Height="76" Width="266" Padding="7" Margin="591,148,0,0">
                        <!--region #Layers-->
                        <dxga:DigitalGaugeControl.Layers>
                            <dxga:DigitalGaugeLayer>
                                <dxga:DigitalGaugeLayer.Presentation>
                                    <dxga:EcoDigitalGaugeBackgroundLayerPresentation/>
                                </dxga:DigitalGaugeLayer.Presentation>
                            </dxga:DigitalGaugeLayer>
                        </dxga:DigitalGaugeControl.Layers>
                        <!--endregion #Layers-->
                        <!--region #Model-->
                        <dxga:DigitalGaugeControl.Model>
                            <dxga:DigitalEcoModel/>
                        </dxga:DigitalGaugeControl.Model>
                        <!--endregion #Model-->
                        <dxga:DigitalGaugeControl.SymbolView>
                            <dxga:FourteenSegmentsView/>
                        </dxga:DigitalGaugeControl.SymbolView>
                    </dxga:DigitalGaugeControl>

                    <Image x:Name="image" HorizontalAlignment="Left" Height="48" Margin="538,163,0,0" VerticalAlignment="Top" Width="48" Source="Assets/Images/Objects/poo.png"/>
                    <dxc:ChartControl x:Name="BedroomLog" HorizontalAlignment="Stretch" Height="257" Margin="10,348,10,0" VerticalAlignment="Top" Width="888">
                        <dxc:ChartControl.Legend>
                            <dxc:Legend/>
                        </dxc:ChartControl.Legend>
                        <dxc:XYDiagram2D>
                            <dxc:XYDiagram2D.AxisX>
                                <dxc:AxisX2D>
                                    <dxc:AxisX2D.DateTimeScaleOptions>
                                        <dxc:ManualDateTimeScaleOptions MeasureUnit="Minute">

                                        </dxc:ManualDateTimeScaleOptions>

                                    </dxc:AxisX2D.DateTimeScaleOptions>
                                </dxc:AxisX2D>
                            </dxc:XYDiagram2D.AxisX>
                            <dxc:LineSeries2D DisplayName="Влажность"/>
                            <dxc:LineSeries2D DisplayName="Температура"/>
                        </dxc:XYDiagram2D>
                    </dxc:ChartControl>

                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
