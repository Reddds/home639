<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HomeModbus"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:rdControls="clr-namespace:RdControls;assembly=RdControls"
        xmlns:tb="http://www.hardcodet.net/taskbar"
        xmlns:commands="clr-namespace:HomeModbus.Commands"
        xmlns:tooltip="clr-namespace:HomeModbus.Tooltip"
        xmlns:controls="clr-namespace:HomeModbus.Controls"
        xmlns:wpfGauge="clr-namespace:WpfGauge;assembly=WpfGauge"
        xmlns:CircularGauge="clr-namespace:CircularGauge;assembly=CircularGauge" 
        xmlns:dxga="http://schemas.devexpress.com/winfx/2008/xaml/gauges"
        xmlns:properties="clr-namespace:HomeModbus.Properties"
        xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/charts"
        x:Class="HomeModbus.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="673.14" Width="920.821" Closed="Window_Closed"
        DataContext="{Binding RelativeSource={RelativeSource Self}}" Closing="Window_Closing" Loaded="Window_Loaded">
    <Grid>
        <TabControl x:Name="MainTabs">
            <TabItem Header="Основное">
                <Grid>
                    <tb:TaskbarIcon x:Name="MyNotifyIcon" ToolTipText="hello world" IconSource="Assets/Images/com_plug.ico">
                        <tb:TaskbarIcon.ContextMenu>
                            <ContextMenu>
                                <TextBlock Text="Hiding the sample window will not close it!" />
                                <MenuItem Header="_Показать [S]"
									Command="{commands:ShowSampleWindowCommand}"
									CommandParameter="{Binding}">
                                    <MenuItem.Icon>
                                        <Image Width="16"
											Height="16"
											Source="/Assets/Images/Add.png" />
                                    </MenuItem.Icon>
                                </MenuItem>

                                <MenuItem Header="_Скрыть [H]"
									Command="{commands:HideSampleWindowCommand}"
									CommandParameter="{Binding}">
                                    <MenuItem.Icon>
                                        <Image Width="16"
											Height="16"
											Source="/Assets/Images/Remove.png" />
                                    </MenuItem.Icon>
                                </MenuItem>
                                <Separator />

                                <MenuItem Header="_Выход [X]"
									Command="{commands:CloseWindowCommand}"
									CommandParameter="{Binding}">
                                    <MenuItem.Icon>
                                        <Image Width="16"
											Height="16"
											Source="/Assets/Images/Close.png" />
                                    </MenuItem.Icon>
                                </MenuItem>
                            </ContextMenu>
                        </tb:TaskbarIcon.ContextMenu>
                        <tb:TaskbarIcon.TrayPopup>
                            <!-- the control will be put into a popup with an explicit DataContext -->
                            <tooltip:FancyPopup />
                        </tb:TaskbarIcon.TrayPopup>
                    </tb:TaskbarIcon>
                    <GroupBox x:Name="GbConnection" HorizontalAlignment="Left" Height="133" Margin="10,10,0,0" VerticalAlignment="Top" Width="231">
                        <GroupBox.Header>
                            <StackPanel Orientation="Horizontal">
                                <Button>
                                    <Button.Style>
                                        <Style TargetType="{x:Type Button}">
                                            <Setter Property="Padding" Value="0"/>
                                            <!--<Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate>
                                                <ContentPresenter Content="{Binding Content}"/>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>-->
                                        </Style>
                                    </Button.Style>
                                    <Rectangle Width="16" Height="16" Stroke="Black">
                                        <Rectangle.Style>
                                            <Style TargetType="{x:Type Rectangle}">
                                                <Setter Property="Fill" Value="#FFF4F4F5"/>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding IsPortOpen}" Value="True">
                                                        <Setter Property="Fill" Value="LimeGreen"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Rectangle.Style>

                                    </Rectangle>

                                </Button>
                                <TextBlock Margin="5,0,0,0"><Run Text="Connection port"/></TextBlock>
                            </StackPanel>
                        </GroupBox.Header>
                        <Grid>
                            <Grid.Style>
                                <Style TargetType="{x:Type Grid}">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding Connected}" Value="True">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="24"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Label Content="Select COM port" />
                            <ListBox Grid.Row="1" Grid.Column="0" Margin="5" x:Name="LbPorts" SelectionChanged="SelectPort" />
                            <Label Grid.Row="0" Grid.Column="1" Content="Baudrate" />
                            <ListBox Grid.Row="1" Grid.Column="1" x:Name="LbBaudRates" SelectionChanged="SelectBaudrate" Margin="5" />


                        </Grid>
                    </GroupBox>


                    <Button x:Name="button" Content="Начать" HorizontalAlignment="Left" Height="27" Margin="170,147,0,0" VerticalAlignment="Top" Width="151" Click="StartWatchingClick">
                        <Button.Style>
                            <Style TargetType="{x:Type Button}">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsListening}" Value="True">
                                        <Setter Property="IsEnabled" Value="False"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Style>
                    </Button>

                    <ListBox x:Name="LbLog" Margin="10,182,10,10.2"/>
                    <CheckBox x:Name="CbLight" Content="Свет" HorizontalAlignment="Left" Margin="10,162,0,0" VerticalAlignment="Top"/>
                    <CheckBox x:Name="CbBathDoor" Content="Дверь в ванной закрыта" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,147,0,0" IsEnabled="False"/>
                    <Button x:Name="BStop" Content="Остановить" IsEnabled="{Binding IsListening}" HorizontalAlignment="Left" Height="27" Margin="326,147,0,0" VerticalAlignment="Top" Width="160" Click="BStop_Click"/>

                </Grid>
            </TabItem>
            <TabItem Header="Коридор">
                <Grid>
                    <GroupBox x:Name="groupBox1" Header="Demo EEPROM" HorizontalAlignment="Left" Height="107" Margin="246,10,0,0" VerticalAlignment="Top" Width="372">
                        <Grid>
                            <rdControls:FilePicker x:Name="FpDemoEeprom" SettingKey="DemoEeprom" Filter="EEPROM|*.eep;*.hex" Margin="0,10,0,0" VerticalAlignment="Top"/>
                            <Button x:Name="button1" Content="Послать программу для Demo" HorizontalAlignment="Left" Height="36" Margin="0,45,0,0" VerticalAlignment="Top" Width="186" Click="button1_Click"/>

                        </Grid>
                    </GroupBox>
                    <GroupBox x:Name="groupBox" Header="Программатор для демо" HorizontalAlignment="Left" Height="192" Margin="10,181,0,0" VerticalAlignment="Top" Width="631">
                        <StackPanel>
                            <StackPanel Orientation="Horizontal" Margin="5">
                                <CheckBox x:Name="CbLed1" VerticalAlignment="Center" IsChecked="True" />
                                <CheckBox x:Name="CbLed2" VerticalAlignment="Center" />
                                <CheckBox x:Name="CbLed3" VerticalAlignment="Center" />
                                <CheckBox x:Name="CbLed4" VerticalAlignment="Center" />
                                <Label VerticalAlignment="Center" Content="Яркость"/>
                                <ComboBox x:Name="CbBrightnes" VerticalAlignment="Center" DisplayMemberPath="Key" SelectedValuePath="Value" Width="70"/>
                                <Label VerticalAlignment="Center" Content="Задержка после"/>
                                <xctk:IntegerUpDown x:Name="IudDelayAfter" Minimum="0" VerticalAlignment="Center" Margin="5,0" Width="60" Maximum="2550" Text="500" DefaultValue="500"/>
                                <RadioButton x:Name="RbDelayMs" VerticalAlignment="Center" IsChecked="True" GroupName="Delays" Content="ms" Checked="RbDelayMs_Checked"/>
                                <RadioButton x:Name="RbDelayS" VerticalAlignment="Center" Margin="5,0" GroupName="Delays" Content="s" Checked="RbDelayS_Checked"/>
                                <Button Height="23" Width="120" Margin="20,0" Click="Button_Click_1" Content="Добавить команду"/>
                                <Button Height="23" VerticalAlignment="Center" Content="&lt;-" ToolTip="Удалить последнюю комманду" Click="DeleteLastDemoCommand"/>
                            </StackPanel>
                            <ListBox x:Name="LbDemoCommands" Height="100"/>
                            <StackPanel Orientation="Horizontal" Margin="5">
                                <Label Content="Осталось места (байт):"/>
                                <Label x:Name="LDemoBytesLeft" FontWeight="Bold" Content="256"/>
                                <Button Click="SaveDemo" Margin="5" Height="23" Width="150" Content="Сохранить для передачи"/>

                            </StackPanel>
                        </StackPanel>
                    </GroupBox>
                </Grid>
            </TabItem>
            <TabItem Header="Ванна">
                <Grid>
                    <controls:ObjectButton x:Name="OBathBulb" Width="70" Height="70" HorizontalAlignment="Left"
						VerticalAlignment="Top"
						EnabledChecked="Assets/Images/Objects/LightBulbOn.png"
						EnabledUnchecked="Assets/Images/Objects/LightBulbOff.png" Margin="51,34,0,0"/>

                    <controls:ObjectButton x:Name="OBathDoor" Width="70" Height="70" HorizontalAlignment="Left"
						VerticalAlignment="Top"
						EnabledChecked="Assets/Images/Objects/DoorClosed.png"
						EnabledUnchecked="Assets/Images/Objects/DoorOpened.png"  Margin="148,47,0,0"/>

                </Grid>
            </TabItem>
            <TabItem Header="Спальня">
                <Grid>
                    <Button x:Name="button2" Content="Установить текущее время" Click="SetCurrentTime" HorizontalAlignment="Left" Height="27" Margin="10,10,0,0" VerticalAlignment="Top" Width="262"/>
                    <!--

                    <wpfGauge:Gauge 
						FontSize="12" AutoScale="False" Value="20"
						x:Name="GBedroomTemp" MajorTickCount="10" MinorTickCount="5" MinValue="0" MaxValue="50" 
						HorizontalAlignment="Left" StartAngle="-65" EndAngle="247" MajorTickMarkColor="LightYellow" Margin="10,88,0,0" Grid.Row="0" VerticalAlignment="Top" Height="200" Width="200">
